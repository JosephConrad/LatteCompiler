int ifac(int n) { return ifac2f(n,1); }

int ifac2f(int l, int h) {
        if (l == 0) return 1;
        else if (l == 1) return 1;
        else
            return ifac2f(l-1,1) + ifac2f(l-2,1); 
}

int rfac(int n) {
    if (n == 0)
      return 1;
    else
      return n * rfac(n-1);
}
